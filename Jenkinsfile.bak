node {
       stage('pulling code from git'){
        checkout scm
       }
  stage('Build Image'){
        sh '/opt/gradle/gradle-5.2.1/bin/gradle build'
  }
  
  tage('Build Image'){
        sh 'sudo docker build -t veeramatli/accountservices-api:${BUILD_NUMBER} .'
        sh 'docker tag veeramatli/accountservices-api:${BUILD_NUMBER} veeramatli/accountservices-api:latest'
  }
  stage('Push Image'){
        sh 'docker login -u idexcelinterns -p kutty170065'
        sh 'docker push idexcelinterns/saidev:latest'
  }
 
}




